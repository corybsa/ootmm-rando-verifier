{
  "name": "angular-template",
  "version": "0.0.0",
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run dev\" \"npm run dev-server\"",
    "build": "ng build -c prod",
    "build-dev": "ng build -c dev --aot",
    "build-prod": "ng build -c prod --aot --build-optimizer --optimization",
    "build-server": "tsc -p tsconfig.server.json",
    "watch": "ng build --watch -c dev",
    "test": "ng test",
    "dev": "ng build -c dev --watch --aot",
    "dev-server": "tsc-watch -p tsconfig.server.json --onFirstSuccess \"npm run copy-files\" --onSuccess \"node dist-server/server.js\"",
    "copy-files": "npm run copy-config && npm run copy-email-templates",
    "copy-config": "xcopy /Y .\\server\\config\\.env .\\dist-server\\config\\",
    "copy-email-templates": "xcopy /E/Y .\\server\\email-templates\\* .\\dist-server\\email-templates\\",
    "stripe-listener": "~/Downloads/stripe.exe listen --forward-to localhost:8081/stripehook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.6",
    "@angular/cdk": "^17.0.3",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.3.11",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.0.6",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/router": "^17.3.11",
    "@angular/service-worker": "^17.3.11",
    "@auth0/angular-jwt": "^5.2.0",
    "@aws-sdk/client-rekognition": "^3.614.0",
    "@aws-sdk/client-ses": "^3.614.0",
    "@aws-sdk/client-sns": "^3.614.0",
    "@aws-sdk/client-sqs": "^3.614.0",
    "@ericblade/quagga2": "^1.8.4",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/store": "^17.0.1",
    "@ngrx/store-devtools": "^17.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "canvas-confetti": "^1.9.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "~16.0.0",
    "easy-speech": "^2.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "express-session": "^1.18.0",
    "fraction.js": "^4.3.7",
    "fuzzysort": "^1.9.0",
    "helmet": "^5.1.1",
    "http": "0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.7.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongodb": "^5.9.2",
    "ngx-barcode-scanner": "^14.0.1",
    "ngx-extended-pdf-viewer": "^18.1.14",
    "ngx-material-timepicker": "^13.1.1",
    "ngx-number-spinner": "^2.2.1",
    "ngx-webcam": "^0.4.1",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "rxjs": "^7.8.1",
